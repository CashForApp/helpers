{
  "name": "phpcraftsman/helpers",
  "type": "library",
  "description": "Assistant package",
  "license": "GPL-3.0",
  "minimum-stability": "stable",
  "homepage": "https://github.com/phpcraftsman/helpers",
  "authors": [
    {
      "name": "Sergey Andrianov",
      "email": "batisska@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "orchestra/testbench": "^6.0",
    "laravel/legacy-factories": "^1.0.4"
  },
  "autoload": {
    "psr-4": {
      "PhpCraftsman\\": "src/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Factories\\": "tests/factories/",
      "Database\\Factories\\":"database/factories/",
      "Database\\Factories\\PhpCraftsman\\":"database/factories/"
    }
  }
}
